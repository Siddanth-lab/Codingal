import random
playing=True
number=str(random.randint(10,20))

print("I will say one number from 0 trough 9 and you will try to guess it.")
print("The game ends when you reach 1 hero!")
while playing:
    guess=input("Show me your best guess!:")
    if number==guess:
        print("You win the game!")
    else:
        print("That is not the number try again!")
